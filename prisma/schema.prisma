// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Donation {
  id         String    @id @map("_id")
  repo       String
  stars      Int
  amount     String
  createdAt  DateTime  @default(now())
  claimedAt  DateTime?
}
